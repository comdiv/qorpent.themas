#160
thema base, "Базовый класс тем", abst=true :
	layout=eco 
	out.template=empty
	input.template=empty 
thema report,"Базовый класс одиночных отчетов",  abst=true :
	import base
	group=adv_reports
thema forma,"Базовый класс одиночных форм", abst=true :
	import base
	group=adv_forms
	
group graphics, "Графики"
thema graphic, "Базовая тема для графиков", abst=true, group = graphics :
	comment = """
               Подключает движок генерации графиков и базовые параметры оформелния графиков
        """ 
	datagenerator = graph_default
	graphtitle = ""
	postgraph = "${id}"
	layout = eco
	out.template = empty
	out code="aA", role="ANALYTIC" :
		isvalid
		ask graphtype
		ask graphwidth
		ask graphheight
		ask graphxlabel
		ask graphylabel
		ask graphy2label
		ask graphshowvalues
		ask graphroundededge
		ask graphformatnumberscale
		ask graphpalette
		ask graphtitle
		ask graphsubtitle
		ask axisymin
		ask axisymax
		
		param viewname : "std/vdr"
		param generatorname : "std/vdr"
		
		useset @pregraph
		show graph_head, at=head
		show graph
		useset @postgraph
		ask graphview : "${datagenerator}"























































































































































































































































































































































































































