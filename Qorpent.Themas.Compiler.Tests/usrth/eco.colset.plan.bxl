#План (форма)
#------------------------------------------------------------------------------------
subset cs_plan_form_1, "Плановая форма" :
	import cs_ozhid_god
	col Б1, "Факт {0}", year = -1, period=4, fixed, forperiods="252,253,254", format=_INT

	col PLAN, "ТПФП {0} утв.", matrixid="PLAN301", period=301, forperiods="301,251,252,253,254,303,306,309", fixed, format="#,###.##"
