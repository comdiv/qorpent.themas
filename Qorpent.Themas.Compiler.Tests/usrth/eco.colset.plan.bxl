	col PLANC, "Корр. за II кв {0}", period="32" , matrixid="KORR2", forperiods="252,253,254", fixed, format=_INT
	col PLANC, "Корр. за III кв {0}", period="33", matrixid="KORR3", forperiods="253,254", fixed, format=_INT
	col PLANC, "Корр. за IV кв {0}", period="34", matrixid="KORR4", forperiods="254", fixed, format=_INT
	col PLANCORRECTED, "ТПФП {0} утв. с уч. корр.", matrixformula="PLAN301,KORR1", matrixformulatype="sum", forperiods="251", formula=" @PLAN.P301? + @PLANC.P31? ", fixed, format=_INT
	col PLANCORRECTED, "ТПФП {0} утв. с уч. корр.", matrixformula="PLAN301,KORR1,KORR2", matrixformulatype="sum", forperiods="252", formula=" @PLAN.P301? + @PLANC.P(31,32)? ", fixed, format=_INT
	col PLANCORRECTED, "ТПФП {0} утв. с уч. корр.", matrixformula="PLAN301,KORR1,KORR2,KORR3", matrixformulatype="sum", forperiods="253", formula=" @PLAN.P301? + @PLANC.P(31,32,33)? ", fixed, format=_INT
	col PLANCORRECTED, "ТПФП {0} утв. с уч. корр.", matrixformula="PLAN301,KORR1,KORR2,KORR3,KORR4", matrixformulatype="sum", forperiods="254", formula=" @PLAN.P301? + @PLANC.P(31,32,33,34)? ", fixed, format=_INT

	import cs_kvplan
		

