	col PLANCONTR, "Откл. от утв. ТПФП", matrixformula="PLAN250,-PLAN301", matrixformulatype="sum", forperiods="251", formula=" @PLAN.P251? - @PLAN.P301? ", fixed, format=_INT
	col PLANCONTR, "Откл. от утв. ТПФП", matrixformula="PLAN250,-PLAN301", matrixformulatype="sum", forperiods="252", formula=" @PLAN.P252? - @PLAN.P301? ", fixed, format=_INT
	col PLANCONTR, "Откл. от утв. ТПФП", matrixformula="PLAN250,-PLAN301", matrixformulatype="sum", forperiods="253", formula=" @PLAN.P253? - @PLAN.P301? ", fixed, format=_INT
	col PLANCONTR, "Откл. от утв. ТПФП", matrixformula="PLAN250,-PLAN301", matrixformulatype="sum", forperiods="254", formula=" @PLAN.P254? - @PLAN.P301? ", fixed, format=_INT
	
