thema material_balans_base, "Базовая форма по движению ценностей", group="cost_grp", parent = "material_balans", rootrow = 'm150', role="ADMIN", abst :
	import assoieco
	import forprdtest
	usequickupdateA = false
	usequickupdateB = false
	usequickupdateC = false
	activeC = false
	ra_lib2B = ""
	rb_lib2B = ""
	f_libB = ""
	
	lockA = "${code}A"
	lockB = "${code}B"
	mbtype = ""
	selfform = "false"
	reportset Aa :
		use viewname : "raw/default"
		use generatorname : "raw/default"
		ask usemes 
		ask usesng : false
		ask planfact : "FACT"
		ask useotkl : false
		ask use_metal_filter
		ask show_summs_only
		ask metal_filter_au
		ask metal_filter_ag
		ask metal_filter_cu
		ask metal_filter_zn
		ask metal_filter_pb
		ask metal_filter_smt
		ask metal_filter_sod
	reportset Ab :
		use viewname : "raw/default"
		use generatorname : "raw/default"
		ask usemes 
		ask usesng : false
		ask planfact : "FACT"
		ask useotkl : false
	reportset Ba :
		use viewname : "raw/default"
		use generatorname : "raw/default"
		ask use_metal_filter
		ask show_summs_only
		ask metal_filter_au
		ask metal_filter_ag
		ask metal_filter_cu
		ask metal_filter_zn
		ask metal_filter_pb
		ask metal_filter_smt
		ask metal_filter_sod
		hide useotkl
	reportset Bb :
		use viewname : "raw/default"
		use generatorname : "raw/default"

