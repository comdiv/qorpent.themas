#====================================================================================
#Расходы на медицинское обслуживание
#------------------------------------------------------------------------------------
assoisoc med,  "Расходы на медицинское обслуживание", socroot=200 :
	import fordesign
	formset _ФОРМА_ПО_ФАКТУ :
		col "2KFSEB", "Себестоимость"
		col "0KF", "Прибыль"
		col "2KFDMSP", "ДМС перечислено"
		col "2KFDMSC", "ДМС получено"
		col "2KFFSS", "ФСС"
		col "MEDSUM", "Всего", formula=" @2KFSEB? + @0KF? + @2KFDMSP? + @2KFFSS? " , fixed
	formset B :
		col "2KFSEB", "Себестоимость"
		col "0KF", "Прибыль"
		col "2KFDMSP", "ДМС перечислено"
		col "2KFDMSC", "ДМС получено"
		col "2KFFSS", "ФСС"
		col "MEDSUM", "Всего", formula=" @2KFSEB? + @0KF? + @2KFDMSP? + @2KFFSS? " , fixed
		
