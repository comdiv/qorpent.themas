#------------------------------------------------------------------------------------
subset cs_dkz_report, "Дебиторка и кредиторка (отчет)" :
	col DZn, "Деб. з-ть на {5}", matrixid="SNGDZn-1", period=3, year=-1, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and СНГ and PREDYEAR"
	col DZRn, "Резервы на {5}", matrixid="SNGDZRn-1", year=-1, period=3, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and RESERV and СНГ and PREDYEAR"
#	col DZSRn, "Деб. з-ть с уч. резервов {5}", matrixid="SNGDZSRn-1", period=3, year=-1, formula="", forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and СНГ and PREDYEAR"
	col KZn, "Кр. з-ть на {5}", matrixid="SNGKZn-1", period=3, year=-1, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="KZ and СНГ and PREDYEAR"

	col DZn, "Деб. з-ть на {4}", matrixid="MESDZn", forperiods="_ТОЛЬКО_МЕС", condition="DZ and BYMONTH"
	col DZRn, "Резервы на {4}", matrixid="MESDZRn", forperiods="_ТОЛЬКО_МЕС", condition="DZ and RESERV and BYMONTH"
	col KZn, "Кр. з-ть на {4}", matrixid="MESKZn", forperiods="_ТОЛЬКО_МЕС", condition="KZ and BYMONTH"

	col DZn, "Деб. з-ть на {4}", matrixid="SNGDZn", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and СНГ"
	col DZRn, "Резервы на {4}", matrixid="SNGDZRn", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and RESERV and СНГ"
	col KZn, "Кр. з-ть на {4}", matrixid="SNGKZn", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="KZ and СНГ"

	col DZk, "Деб. з-ть на {6}", matrixid="SNGDZk", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and ( BYMONTH or СНГ )"
	col DZRk, "Резервы на {6}", matrixid="SNGDZRk", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and RESERV and ( BYMONTH or СНГ )"
	col KZk, "Кр. з-ть на {6}", matrixid="SNGKZk", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="KZ and ( BYMONTH or СНГ )"

	col OTKL1, "Изм. деб. з-ти за {2} {0}", matrixformula="SNGDZk,-MESDZn", matrixformulatype="sum", formula=" @DZk? - @DZn? ", forperiods="_ТОЛЬКО_МЕС", condition="DZ and OTKL and BYMONTH"
	col OTKL2, "Изм. резервов за {2} {0}", matrixformula="SNGDZRk,-MESDZRn", matrixformulatype="sum", formula=" @DZRk? - @DZRn? ", forperiods="_ТОЛЬКО_МЕС", condition="DZ and RESERV and OTKL and BYMONTH"
	col OTKL3, "Изм. кред. з-ти за {2} {0}", matrixformula="SNGKZk,-MESKZn", matrixformulatype="sum", formula=" @KZk? - @KZn? ", forperiods="_ТОЛЬКО_МЕС", condition="KZ and OTKL and BYMONTH"

	col OTKL4, "Изм. деб. з-ти за {2} {0}", matrixformula="SNGDZk,-SNGDZn", matrixformulatype="sum", formula=" @DZk.P@_СУММЕСЯЦ? - @DZn.P@_СУММЕСЯЦ? ", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and OTKL and СНГ"
	col OTKL5, "Изм. резервов за {2} {0}", matrixformula="SNGDZRk,-SNGDZRn", matrixformulatype="sum", formula=" @DZRk.P@_СУММЕСЯЦ? - @DZRn.P@_СУММЕСЯЦ? ", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="DZ and RESERV and OTKL and СНГ "
	col OTKL6, "Изм. кред. з-ти за {2} {0}", matrixformula="SNGKZk,-SNGKZn", matrixformulatype="sum", formula=" @KZk.P@_СУММЕСЯЦ? - @KZn.P@_СУММЕСЯЦ? ", period=_СУММЕСЯЦ, forperiods="_ТОЛЬКО_МЕС,_МЕС_СНГ,_ОЖИД_СНГ", condition="KZ and OTKL and СНГ "


