	col PLAN, "План 2 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=303, forperiods="12", format=_INT, condition="PLAN", formula="@PLAN.P303? / 3 * 2", style=_STYLE_PLAN
	col PLAN, "План I кв {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=303, forperiods="1,13,403", format=_INT, condition="PLAN", style=_STYLE_PLAN

	col PLANKV, "План 4 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=306, forperiods="14", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? / 3", style=_STYLE_PLAN
	col PLANKV, "План 5 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=306, forperiods="15", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? / 3 * 2", style=_STYLE_PLAN
	col PLANKV, "План I пг {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=306, forperiods="2,16,406", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? ", style=_STYLE_PLAN

	col PLANKV, "План 7 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=309, forperiods="17", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? / 3 ", style=_STYLE_PLAN
	col PLANKV, "План 8 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=309, forperiods="18", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? / 3 * 2 ", style=_STYLE_PLAN
	col PLANKV, "План 9 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=309, forperiods="3,19,409", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? ", style=_STYLE_PLAN

	col PLANKV, "План 10 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=301, forperiods="110", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? + @DELTAKV.P301? / 3", style=_STYLE_PLAN
	col PLANKV, "План 11 мес. {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=301, forperiods="111", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? + @DELTAKV.P301? / 3 * 2", style=_STYLE_PLAN
	col PLANKV, "План {0}", customCode="AGGPLAN2", matrixid="AGGPLAN2", period=301, forperiods="4,112,401", format=_INT, condition="KVKURS and PLAN", formula="@PLAN.P303? + @DELTAKV.P306? + @DELTAKV.P309? + @DELTAKV.P301?", style=_STYLE_PLAN

