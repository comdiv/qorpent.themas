subset cs_dkz_control, "Дебиторка и кредиторка (контроль)" :
	col DZk, "Деб. з-ть кон. 12 мес. {0}", customCode="DZn4-1", year=-1, period=4, forperiods=1, fixed
	col KZk, "Кред. з-ть кон. 12 мес. {0}", customCode="KZn4-1", year=-1, period=4, forperiods=1, fixed
	col DZn, "Деб. з-ть нач. 3 мес. {0}", customCode="DZn1", period=1, forperiods=1, fixed
	col KZn, "Кред. з-ть нач. 3 мес. {0}", customCode="KZn1", period=1, forperiods=1, fixed
	col OTKL1, "Отклонение деб. з-ти", formula=" @DZn.P1? - @DZk.Y-1.P4? ", forperiods=1
	col OTKL2, "Отклонение кред. з-ти", formula=" @KZn.P1? - @KZk.Y-1.P4? ", forperiods=1

	col DZn, "Деб. з-ть нач. 9 мес. {0}", customCode="DZn3", period=3, forperiods=4, fixed
	col KZn, "Кред. з-ть нач. 9 мес. {0}", customCode="KZn3", period=3, forperiods=4, fixed
	col DZn, "Деб. з-ть нач. 12 мес. {0}", customCode="DZn4", period=4, forperiods=4, fixed
	col KZn, "Кред. з-ть нач. 12 мес. {0}", customCode="KZn4", period=4, forperiods=4, fixed
	col OTKL3, "Отклонение деб. з-ти", formula=" @DZn.P4? - @DZn.P3? ", forperiods=4
	col OTKL4, "Отклонение кред. з-ти", formula=" @KZn.P4? - @KZn.P3? ", forperiods=4

