#====================================================================================
#Колонки месячных планов
#------------------------------------------------------------------------------------
subset cs_operplan_mes_report, "Месячные планы" :
	col PLAN, "План {2} {0}", customCode="MONTHPLAN", matrixid="MONTHPLAN", forperiods="12,13", format=_INT, condition="PLAN", formula=" @PLAN.P303? / 3", style=_STYLE_PLAN
	col PLANKV, "План {2} {0}", customCode="MONTHPLAN", matrixid="MONTHPLAN", forperiods="14,15,16", format=_INT, condition="PLAN", formula=" ( @PLAN.P306? - @PLAN.P303? ) / 3", style=_STYLE_PLAN
	col PLANKV, "План {2} {0}", customCode="MONTHPLAN", matrixid="MONTHPLAN", forperiods="17,18,19", format=_INT, condition="PLAN", formula=" ( @PLAN.P309? - @PLAN.P306? ) / 3 ", style=_STYLE_PLAN
	col PLANKV, "План {2} {0}", customCode="MONTHPLAN", matrixid="MONTHPLAN", forperiods="110,111,112", format=_INT, condition="PLAN", formula=" ( @PLAN.P301? - @PLAN.P309? ) / 3", style=_STYLE_PLAN

#====================================================================================
