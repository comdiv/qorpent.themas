#####################################################################################
###################              Типовые наборы колонок             #################
#####################################################################################

#====================================================================================
#Действующий годовой ТПФП
#------------------------------------------------------------------------------------
subset cs_tpfp, "Утвержденный ТПФП" :
	col PLAN, "Утвержд. ТПФП {0}", period=301, fixed, condition=PLAN

#====================================================================================
#Коррективы и изменения ТПФП
#------------------------------------------------------------------------------------
subset cs_tpfp_izm, "Коррективы и изменения ТПФП" :
	col IZM, "в т.ч. изменение к первонач. ТПФП {0}", condition="PLAN,USEIZMTPFP", formula=" @PLAN.P301? - @PLAN.P251? "
	col KORR1, "Корр. ТПФП за {2} {0}", period=1, condition="PLAN,USEKORRTPFP", forperiods="11,12,13,1,403", formula=" @PLANC.P31? "
	col KORR2, "Корр. ТПФП за {2} {0}", period=2, condition="PLAN,USEKORRTPFP", forperiods="14,15,16,2,406", formula=" @PLANC.P31? + @PLANC.P32? "
	col KORR3, "Корр. ТПФП за {2} {0}", period=3, condition="PLAN,USEKORRTPFP", forperiods="17,18,19,3,409", formula=" @PLANC.P31? + @PLANC.P32? + @PLANC.P33? "
	col KORR4, "Корр. ТПФП за {2} {0}", period=4, condition="PLAN,USEKORRTPFP", forperiods="110,111,112,4,401", formula=" @PLANC.P31? + @PLANC.P32? + @PLANC.P33? + @PLANC.P34?"

#====================================================================================
#Колонки плановых показателей нарастающим итогом
#------------------------------------------------------------------------------------
subset cs_plan_sng_form, "Месячный план (нар. итогом)" :
	col PLANSNG1, matrixid="PLANSNG", _ПЛАН, period=_СУММЕСЯЦ, forperiods="1,11,12,13,403", fixed, validation="validate-int-cell-number"
	col PLANSNG2, matrixid="PLANSNG", _ПЛАН, period=_СУММЕСЯЦ, forperiods="2,14,15,16,406", fixed, validation="validate-int-cell-number"
	col PLANSNG3, matrixid="PLANSNG",_ПЛАН, period=_СУММЕСЯЦ, forperiods="3,17,18,19,409", fixed, validation="validate-int-cell-number"
	col PLANSNG4, matrixid="PLANSNG",_ПЛАН, period=_СУММЕСЯЦ, forperiods="4,110,111,112,401", fixed, validation="validate-int-cell-number"
	col PLAN, period=303, forperiods="1,11,12,13,2,14,15,16,403", visible=false
	col PLAN, period=306, forperiods="2,14,15,16,3,17,18,19,403,406", visible=false
	col PLAN, period=309, forperiods="3,17,18,19,4,110,111,112,406,409", visible=false
	col PLAN, period=301, forperiods="4,110,111,112,401", visible=false

#====================================================================================
#Колонки отклонения фактических от плановых показателей нарастающим итогом
#------------------------------------------------------------------------------------
subset cs_otkl_plan_sng_form, "Отклонения факта от плана (нар. итогом)" :
	col AGGOTKL1, _ОТКЛПЛАН, matrixformula="TEKSNG,-PLANSNG", matrixformulatype="sum", period=_СУММЕСЯЦ, forperiods="1,11,12,13,403", condition="OTKL",  formula=" @Б1? - @PLANSNG1? ", fixed
	col AGGOTKL2, _ОТКЛПЛАН, matrixformula="TEKSNG,-PLANSNG", matrixformulatype="sum",period=_СУММЕСЯЦ, forperiods="2,14,15,16,406", condition="OTKL", formula=" @Б1? - @PLANSNG2? ", fixed
	col AGGOTKL3, _ОТКЛПЛАН, matrixformula="TEKSNG,-PLANSNG", matrixformulatype="sum",period=_СУММЕСЯЦ, forperiods="3,17,18,19,409", condition="OTKL", formula=" @Б1? - @PLANSNG3? ", fixed
	col AGGOTKL4, _ОТКЛПЛАН, matrixformula="TEKSNG,-PLANSNG", matrixformulatype="sum",period=_СУММЕСЯЦ, forperiods="4,110,111,112,401", condition="OTKL", formula=" @Б1? - @PLANSNG4? ", fixed
	
	col AGGOTKL1P, "_ОТКЛПЛАН, %", matrixformula="TEKSNG,PLANSNG", matrixformulatype="proc", period=_СУММЕСЯЦ, forperiods="1,11,12,13,403", format=_DEC1, condition="OTKL",  formula="( @Б1? - @PLANSNG1? )/ @PLANSNG1? * 100", fixed:
		checkrule action="|>=|", value="15", cellstyle=";color :red;"
		checkrule action="|<-=>|", value="5", value2="15", cellstyle=";color :blue;"
		checkrule action="|<->|", value="0", value2="5", cellstyle=";color :green;"
	col AGGOTKL2P,  "_ОТКЛПЛАН, %", matrixformula="TEKSNG,PLANSNG", matrixformulatype="proc",period=_СУММЕСЯЦ, forperiods="2,14,15,16,406", format=_DEC1,condition="OTKL", formula=" ( @Б1? - @PLANSNG2? )/ @PLANSNG2? * 100 ", fixed:
		checkrule action="|>=|", value="15", cellstyle=";color :red;"
		checkrule action="|<-=>|", value="5", value2="15", cellstyle=";color :blue;"
		checkrule action="|<->|", value="0", value2="5", cellstyle=";color :green;"
	col AGGOTKL3P,  "_ОТКЛПЛАН, %", matrixformula="TEKSNG,PLANSNG", matrixformulatype="proc",period=_СУММЕСЯЦ, forperiods="3,17,18,19,409", format=_DEC1,condition="OTKL", formula="( @Б1? - @PLANSNG3? )/ @PLANSNG3? * 100", fixed:
		checkrule action="|>=|", value="15", cellstyle=";color :red;"
		checkrule action="|<-=>|", value="5", value2="15", cellstyle=";color :blue;"
		checkrule action="|<->|", value="0", value2="5", cellstyle=";color :green;"
	col AGGOTKL4P,  "_ОТКЛПЛАН, %", matrixformula="TEKSNG,PLANSNG", matrixformulatype="proc",period=_СУММЕСЯЦ, forperiods="4,110,111,112,401", format=_DEC1,condition="OTKL", formula="( @Б1? - @PLANSNG4? )/ @PLANSNG4? * 100", fixed:
		checkrule action="|>=|", value="15", cellstyle=";color :red;"
		checkrule action="|<-=>|", value="5", value2="15", cellstyle=";color :blue;"
		checkrule action="|<->|", value="0", value2="5", cellstyle=";color :green;"
	
	

#====================================================================================
#Колонки ожидаемых форм ввода
#------------------------------------------------------------------------------------
subset cs_pred_year_sng, "Предыдущий год с начала года" :
	col Б1, _ФАКТ, customCode=PREVYEAR, period=_ЯНВ, forperiods="_ЯНВ", year=-1, fixed, condition="PREDYEAR and FACT"
	col Б1, _ФАКТ, customCode=PREVYEAR, period=_СУММЕСЯЦ, forperiods="_НЕ_ЯНВ,_МЕС_СНГ", year=-1, fixed, condition="PREDYEAR and FACT"
	col Б1, _ФАКТ, customCode=PREVYEAR, period=1, forperiods=403, year=-1, fixed, condition="PREDYEAR and FACT"
	col Б1, _ФАКТ, customCode=PREVYEAR, period=2, forperiods=406, year=-1, fixed, condition="PREDYEAR and FACT"
	col Б1, _ФАКТ, customCode=PREVYEAR, period=3, forperiods=409, year=-1, fixed, condition="PREDYEAR and FACT"
	col Б1, _ФАКТ, customCode=PREVYEAR, period=4, forperiods=401, year=-1, fixed, condition="PREDYEAR and FACT"

#====================================================================================
#Колонки ожидаемых форм ввода
#------------------------------------------------------------------------------------
subset cs_nalog_pred_year_sng, "Предыдущий год с начала года (налоги)" :
	col Pd, _ФАКТ, customCode=PREVYEAR, period=_ЯНВ, forperiods="_ЯНВ", year=-1, fixed, condition="PREDYEAR and FACT"
	col Pd, _ФАКТ, customCode=PREVYEAR, period=_СУММЕСЯЦ, forperiods="_НЕ_ЯНВ,_МЕС_СНГ", year=-1, fixed, condition="PREDYEAR and FACT"
	col Pd, _ФАКТ, customCode=PREVYEAR, period=1, forperiods=403, year=-1, fixed, condition="PREDYEAR and FACT"
	col Pd, _ФАКТ, customCode=PREVYEAR, period=2, forperiods=406, year=-1, fixed, condition="PREDYEAR and FACT"
	col Pd, _ФАКТ, customCode=PREVYEAR, period=3, forperiods=409, year=-1, fixed, condition="PREDYEAR and FACT"
	col Pd, _ФАКТ, customCode=PREVYEAR, period=4, forperiods=401, year=-1, fixed, condition="PREDYEAR and FACT"

#====================================================================================
#Колонки ожидаемых форм ввода (расчетно)
#------------------------------------------------------------------------------------
subset cs_ozhid_calc, "Ожидаемая форма (расчетно)" :
	col Б1, _ФАКТ, matrixid="FACTSNGCALC", period=25, forperiods=406, format=_INT, fixed, forgroup="!PR_KVART"
#	col PLAN6, "План май-июнь {0}", matrixid="PLANMESCALC", period=16, forperiods=406, format=_INT, formula=" ( @PLAN.P306? - @PLAN.P303? ) / 3 * 2", forgroup="!PR_KVART"
	col PLAN6, _ПЛАН, matrixid="PLANMESCALC", period=16, forperiods=406, format=_INT, formula=" ( @PLAN.P306? - @PLAN.P303? ) / 3 ", forgroup="!PR_KVART"
	col OZHIDCALC, "Ожид. {2} {0} (расчетно)", matrixid="OZHIDCALC", matrixformula="FACTSNGCALC,PLANMESCALC", matrixformulatype="sum", period=406, forperiods=406, format=_INT, forgroup="!PR_KVART"

	col Б1, _ФАКТ, matrixid="FACTSNGCALC", period=1, forperiods=406, format=_INT, fixed, forgroup="PR_KVART"
	col PLAN6, "План II кв. {0}", matrixid="PLANKVCALC", period=2, forperiods=406, format=_INT, formula=" @PLAN.P306? - @PLAN.P303? ", forgroup="PR_KVART"
	col OZHIDCALC, "Ожид. {2} {0} (расчетно)", matrixid="OZHIDCALC", matrixformula="FACTSNGCALC,PLANKVCALC", matrixformulatype="sum", period=406, forperiods=406, format=_INT, forgroup="PR_KVART"

#====================================================================================
#Колонки ожидаемых форм ввода (расчетно)
#------------------------------------------------------------------------------------
subset cs_ozhid_calc_kvart, "Ожидаемая форма (расчетно за квартал)" :
	col Б1, _ФАКТ, matrixid="FACTSNGCALC", period=1, forperiods=406, format=_INT, fixed
	col PLAN6, "План II кв. {0}", matrixid="PLANKVCALC", period=2, forperiods=406, format=_INT, formula=" @PLAN.P306? - @PLAN.P303? "
	col OZHIDCALC, "Ожид. {2} {0} (расчетно)", matrixid="OZHIDCALC", matrixformula="FACTSNGCALC,PLANKVCALC", matrixformulatype="sum", period=406, forperiods=406, format=_INT

#====================================================================================
#Колонки ожидаемых форм ввода
#------------------------------------------------------------------------------------
subset cs_ozhid, "Ожидаемая форма" :
	col Б1, _ОЖИД, matrixid="TEKSNG", forperiods=_ОЖИД_СНГ, validation="validate-int-cell-number", controlpoint=true

#====================================================================================
#Колонки ожидаемых форм ввода
#------------------------------------------------------------------------------------
subset cs_ozhid_calc_otkl, "Ожидаемая форма (отклонение)" :
	col OZHIDCALCOTKL, "Откл. ожид. от расчетно", forperiods="406", matrixformula="TEKSNG,-OZHIDCALC", matrixformulatype="sum"

#====================================================================================
#Контроль оборотов при вводе данных
#------------------------------------------------------------------------------------
subset cs_contr_ob_form, "Контроль месячных оборотов" :
	col KONTROB, _CONTR_OB, matrixformula="PREDSNG,TEK,-TEKSNG", matrixformulatype="sum", forperiods=_НЕ_ЯНВ, formula=" @Б1.P@_ПРЕД_СУММЕСЯЦ? + @Б1? - @Б1.P@_СУММЕСЯЦ? ", forgroup="!PR_KVART" :
		checkrule action="|<>|", value="0", cellstyle=";color :red; background : yellow;"

#====================================================================================
#Колонки для месячных форм ввода
#------------------------------------------------------------------------------------
subset cs_month_form, "Месячная форма" :
	import cs_pred_year_sng
	import cs_tpfp
	import cs_plan_sng_form
	col Б1, _ФАКТ, matrixid="PREDSNG", period=_ПРЕД_СУММЕСЯЦ, forperiods=_НЕ_ЯНВ, fixed, validation="validate-int-cell-number", forgroup="!PR_KVART"
	col Б1, _ФАКТ, matrixid="TEK", forperiods=_ТОЛЬКО_МЕС, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART"
	col Б1, _ФАКТ, matrixid="TEKSNG", period=_СУММЕСЯЦ, forperiods="_НЕ_ЯНВ,_МЕС_СНГ", auto, fixed, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART"
	col Б1, _ФАКТ, period=_СУММЕСЯЦ, forperiods=_МЕС_СНГ, validation="validate-int-cell-number", controlpoint=true, forgroup="PR_KVART"
	col Б1, "_ФАКТ (ADMIN)", period=_СУММЕСЯЦ, forperiods="_НЕ_ЯНВ", auto, controlpoint=true, forrole="ADMIN",  forgroup="!PR_KVART"
	import cs_ozhid_calc
	import cs_ozhid
	import cs_ozhid_calc_otkl
	import cs_otkl_plan_sng_form
	import cs_contr_ob_form

