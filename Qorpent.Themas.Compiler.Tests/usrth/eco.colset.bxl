	col КОСТ3, "Контроль ревизий = 0",  forperiods=_ОЖИД_СНГ, fixed , formula=" @Б1.P1? + @REVISION? - @Б1? "
	col КОСТ4, "Откл. от 31.12 пред. года",  forperiods="_ЯНВ,_НЕ_ЯНВ,1,2,3,4", fixed , formula=" @Б1.P@_СУММЕСЯЦ? - @Б2.Y-1.P4? ", validation="validate-int-cell-number"

	col Б1ВО, "_НА_НАЧАЛО вн. об.", forperiods=_ЯНВ, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б1ВО, "_НА_НАЧАЛО вн. об.", forperiods=_НЕ_ЯНВ, auto, fixed, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б2ВО, "_НА_КОНЕЦ вн. об.", forperiods="_ЯНВ,_НЕ_ЯНВ", validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б1ВО, "_НА_НАЧАЛО вн. об.", period= _СУММЕСЯЦ,  forperiods="_НЕ_ЯНВ,_МЕС_СНГ",  auto, fixed, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б2ВО, "_НА_КОНЕЦ вн. об.", period= _СУММЕСЯЦ ,  forperiods="_НЕ_ЯНВ,_МЕС_СНГ",  auto, fixed, validation="validate-int-cell-number", controlpoint=true, forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"

	col Б1ВО, "_НА_НАЧАЛО вн. об.", period= _СУММЕСЯЦ, forperiods="1", validation="validate-int-cell-number", controlpoint=true, forgroup="PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б1ВО, "_НА_НАЧАЛО вн. об.", period= _СУММЕСЯЦ, forperiods="2,3,4", auto, fixed, validation="validate-int-cell-number", controlpoint=true, forgroup="PR_KVART,CONS_FIN", forrole="ADMIN"
	col Б2ВО, "_НА_КОНЕЦ вн. об.", period= _СУММЕСЯЦ, forperiods="1,2,3,4", validation="validate-int-cell-number", controlpoint=true, forgroup="PR_KVART,CONS_FIN", forrole="ADMIN"

	col КОСТ, _CONTR_OST,  forperiods=_НЕ_ЯНВ, fixed , formula=" @Б2ВО.P@_СУММЕСЯЦ? - @Б2ВО? + @Б2ВО.P@_ПРЕД_СУММЕСЯЦ? - @Б1ВО?", validation="validate-int-cell-number", forgroup="!PR_KVART,CONS_FIN", forrole="ADMIN"

