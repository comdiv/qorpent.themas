#------------------------------------------------------------------------------------
subset cs_calc_month_form, "Месячная форма (расчетные с н.г.)" :
	import cs_tpfp
	import cs_plan_sng_form
	col Б1, _ФАКТ, period=-502, forperiods=_НЕ_ЯНВ, fixed
	col Б1, _ФАКТ, validation="validate-int-cell-number", controlpoint=true
	col Б1, _ФАКТ, period=-501, forperiods=_НЕ_ЯНВ, fixed
